{
  "name": "investing-server",
  "version": "0.0.1",
  "description": "Awesome project developed with TypeORM.",
  "devDependencies": {
    "@types/node": "^14.14.37",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "@types/cli-progress": "^3.9.1",
    "axios": "^0.21.1",
    "cli-progress": "^3.9.0",
    "date-fns": "^2.20.1",
    "dotenv": "^8.2.0",
    "fastify": "^3.14.1",
    "fuse.js": "^6.4.6",
    "mysql": "^2.14.1",
    "reflect-metadata": "^0.1.10",
    "typeorm": "0.2.31"
  },
  "scripts": {
    "start": "ts-node src/index.ts | pino-pretty -c -t",
    "start:dev": "ts-node-dev --respawn --transpile-only src/index.ts | pino-pretty",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "start:search:dev": "ts-node-dev --respawn --transpile-only src/search/index.ts",
    "syncbot": "ts-node src/sync/index.ts",
    "initialize": "ts-node src/init.ts"
  },
  "workspaces": {
    "nohoist": [
      "typeorm/**",
      "typeorm"
    ]
  }
}
